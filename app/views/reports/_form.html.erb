<%= simple_form_for(@report) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :user %>
    <%= f.input :id_number %>
    <%= f.input :id_type %>
    <%= f.input :country_code %>
    <%= f.input :activity %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
